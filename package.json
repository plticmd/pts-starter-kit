  "bugs": {
    "url": "https://github.com/williamngan/pts-starter-kit/issues"
  },
  "homepage": "https://ptsjs.orgg",
  "devDependencies": {
    "esbuild": "^0.14.0"
  },
  "dependencies": {
    "pts": "^0.10.6"
  }
}
